<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         prefWidth="500.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <Pane prefHeight="100.0" prefWidth="400.0" style="-fx-background-color: dodgerblue; -fx-background-radius: 15;">
         <children>
              <Label fx:id="dialog" layoutX="14.0" layoutY="10.0" prefHeight="80.0" prefWidth="338.0" text="Label" textOverrun="CLIP" wrapText="true" />
         </children>
      </Pane>
        <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="15.0" right="10.0" />
         </HBox.margin>
         <viewport>
            <Rectangle2D />
         </viewport></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
